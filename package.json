{
  "name": "edurateupb",
  "version": "1.0.0",
  "description": "Foro Académico Universidad Pontificia Bolivariana - Plataforma de evaluación de profesores optimizada con Vite",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "preview": "vite preview",
    "build:prod": "tsc && vite build --mode production",
    "build:report": "tsc && vite build --mode analyze && npx vite-bundle-analyzer dist/stats.html",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "lighthouse:upload": "lhci upload",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,scss,css,html,json}",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "upb",
    "universidad",
    "profesores",
    "evaluacion",
    "academico",
    "foro",
    "vite",
    "scss",
    "optimizacion",
    "bundle"
  ],
  "author": "EduRate UPB Team",
  "license": "MIT",
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@types/node": "^24.3.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "eslint": "^8.55.0",
    "gh-pages": "^6.1.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.69.5",
    "terser": "^5.24.0",
    "typescript": "^5.9.2",
    "vite": "^5.0.8",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/edurateupb.git"
  },
  "homepage": "https://your-username.github.io/edurateupb",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
